<p id="notice"><%= notice %></p>

<h1>Venta</h1>

<table>
  <thead>
    <tr>
      <th>Numero</th>
      <th>Tipo</th>
      <th>Subtotal</th>
      <th>Iva</th>
      <th>Propina</th>
      <th>Total</th>
      <th>Fecha</th>
      <th>Pedido</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venta.each do |ventum| %>
      <tr>
        <td><%= ventum.numero %></td>
        <td><%= ventum.tipo %></td>
        <td><%= ventum.subtotal %></td>
        <td><%= ventum.iva %></td>
        <td><%= ventum.propina %></td>
        <td><%= ventum.total %></td>
        <td><%= ventum.fecha %></td>
        <td><%= ventum.pedido %></td>
        <td><%= link_to 'Show', ventum %></td>
        <td><%= link_to 'Edit', edit_ventum_path(ventum) %></td>
        <td><%= link_to 'Destroy', ventum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ventum', new_ventum_path %>

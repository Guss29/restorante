<p id="notice"><%= notice %></p>

<h1>Insumo Platos</h1>

<table>
  <thead>
    <tr>
      <th>Cantidadusada</th>
      <th>Unidadmedida</th>
      <th>Insumo</th>
      <th>Plato</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @insumo_platos.each do |insumo_plato| %>
      <tr>
        <td><%= insumo_plato.cantidadusada %></td>
        <td><%= insumo_plato.unidadmedida %></td>
        <td><%= insumo_plato.insumo %></td>
        <td><%= insumo_plato.plato %></td>
        <td><%= link_to 'Show', insumo_plato %></td>
        <td><%= link_to 'Edit', edit_insumo_plato_path(insumo_plato) %></td>
        <td><%= link_to 'Destroy', insumo_plato, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Insumo Plato', new_insumo_plato_path %>
